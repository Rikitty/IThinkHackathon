{
  "canisters": {
    "VIBE_backend": {
      "build": "npx azle VIBE_backend",
      "candid": "src/VIBE_backend/VIBE_backend.did",
      "gzip": true,
      "main": "src/VIBE_backend/src/index.ts",
      "tech_stack": {
        "cdk": {
          "azle": {}
        },
        "language": {
          "javascript": {},
          "typescript": {}
        }
      },
      "type": "custom",
      "wasm": ".azle/VIBE_backend/VIBE_backend.wasm"
    },
    "VIBE_frontend": {
      "dependencies": [
        "VIBE_backend"
      ],
      "source": [
        "src/VIBE_frontend/dist"
      ],
      "type": "assets",
      "workspace": "VIBE_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}